      "graphQlUrl": "http://localhost:3000/graphql",
      "dbPopulateUrl": "http://localhost:3000/dbPopulate"
    },
    "ingress": {
      "restUrl": "https://kubernetes.docker.internal",
      "authUrl": "https://kubernetes.docker.internal/auth",
      "graphQlUrl": "https://kubernetes.docker.internal/graphql",
      "dbPopulateUrl": "https://kubernetes.docker.internal/dbPopulate"
    }
  },
  "editor.foldingImportsByDefault": true,
  "javascript.inlayHints.variableTypes.enabled": false,
  "typescript.inlayHints.propertyDeclarationTypes.enabled": true,
  "javascript.inlayHints.parameterTypes.enabled": false,
